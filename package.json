    "remark-rehype": "^11.1.1",
    "rrweb-player": "^1.0.0-alpha.4",
    "shiki": "^0.14.3",
    "source-map": "^0.7.4",
    "unified": "^11.0.5",
    "unist-util-visit": "^4.1.2",
    "zustand": "^4.5.2"
