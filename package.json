    "@huolala-tech/page-spy-plugin-rrweb": "^2.2.3",
    "@huolala-tech/page-spy-types": "^2.2.3",
    "@huolala-tech/react-json-view": "^1.2.5",
    "@huolala-tech/request": "^1.1.2",
    "@types/react-window": "^1.8.8",
    "acorn": "^8.11.3",
    "ahooks": "^3.1.9",
    "antd": "^5.18.3",
    "clsx": "^1.2.1",
    "cmdk": "^1.1.1",
    "copy-to-clipboard": "^3.3.3",
    "dayjs": "1.11.7",
